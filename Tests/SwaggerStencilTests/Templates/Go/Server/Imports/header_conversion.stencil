{% macro convertHeaderParameterHelper parameterName items %}
{% set parameterNameCamel %}{{ parameterName|toCamel }}{% endset %}
{{ parameterNameCamel }}String := headerParameters.Get("{{ parameterName }}")

{% call convertItemFromString parameterNameCamel items %}
{% endmacro %}

{% macro convertHeaderParameter parameterName either %}
{% if either.a %}
{% call convertHeaderParameterHelper parameterName either.a.other.items %}
{% else %}
{% call convertHeaderParameterHelper parameterName either.b.structure.other.items %}
{% endif %}
{% endmacro %}