{% macro extractName parameter %}
{% if parameter.body %}
{{ parameter.body.fixedFields.name }}
{% else %}
{{ parameter.other.fixedFields.name }}
{% endif %}
{% endmacro %}

{% macro extractParameterName either %}
{% if either.a %}
{% call extractName either.a %}
{% else %}
{% call extractName either.b.structure %}
{% endif %}
{% endmacro %}

{% macro parameterName either %}
{% set name %}
{% call extractParameterName either %}
{% endset %}
{{ name }}
{% endmacro %}

{% macro parameterNameCamel either %}
{% set name %}{% call parameterName either %}{% endset %}
{{ name|toCamel }}
{% endmacro %}