{% macro convertQueryParameterHelper fixedFields items %}
{% set parameterName fixedFields.parameterName%}
{% set parameterNameCamel %}{{ parameterName|toCamel }}{% endset %}
q.Set("{{ parameterName }}", {% set parameterNameCamel %})

{% call convertItemFromString parameterNameCamel items %}
{% endmacro %}

{% macro encodeQueryParameter either %}
{% if either.a %}
{% call encodeQueryParameterHelper either.a.other.fixedFields either.a.other.items %}
{% else %}
{% call encodeQueryParameterHelper either.b.structure.other.fixedFields either.b.structure.other.items %}
{% endif %}
{% endmacro %}