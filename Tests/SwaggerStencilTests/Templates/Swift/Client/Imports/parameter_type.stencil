{# Requires schema_string.stencil and items_string.stencil #}

{% macro extractParameterType parameter %}
{% if parameter.body %}
{% call schemaString parameter.body.schema %}
{% else %}
{% call itemsString parameter.other.items %}
{% endif %}
{% endmacro %}

{% macro parameterType either %}
{% if either.a %}
{% call extractParameterType either.a %}
{% else %}
{% call extractParameterType either.b.structure %}
{% endif %}
{% endmacro %}