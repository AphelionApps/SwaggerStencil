{% macro schemaString schema %}
{% if schema.structure %}{{ schema.structure.name }}
{% elif schema.array %}
{% if schema.array.items.one %}[{% call schemaString schema.array.items.one %}]{% endif %}
{% elif schema.string %}String
{% elif schema.number %}Double
{% elif schema.integer %}Int
{% elif schema.boolean %}Bool
{% endif %}{% endmacro %}